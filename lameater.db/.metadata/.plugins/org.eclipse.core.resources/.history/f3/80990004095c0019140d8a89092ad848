package bambino.db.player;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import javax.persistence.GeneratedValue;

@Entity // Tells Hibernate to make a table out of this class
@Table(name="players")
public class Player{
	
    @Id
    @GeneratedValue
    @Column(name="PLAYERID")
    private int playerID;
    
    @Column(name="RK")
    private int rank;
    
    @Column(name="NAME")
    private String name;
    
    @Column(name="TEAM")
    private String team;
    
    @Column(name="AVG")
    private float avg;
    
    @Column(name="RC")
    private float rc;
    
    @Column(name="RC27")
    private float rc27;
    
    @Column(name="ISOP")
    private float isop;
    
    @Column(name="SECA")
    private float seca;
	   
    @Column(name="GB")
    private float gb;
   
    @Column(name="FB")
    private float fb;
   
    @Column(name="GF")
    private float gf;
   
    @Column(name="ABHR")
    private float abhr;
   
    @Column(name="BBPA")
    private float bbpa;
   
    @Column(name="BBK")
    private float bbk;
    
 
    
    //Getters
    //Getter for playerID
    public int getPlayerID() {
		return playerID;
	}
    
    //Setter for playerID
	public void setPlayerID(int playerID) {
		this.playerID = playerID;
	}
	
	//Getter for rank
	public int getRank() {
		return rank;
	}

	//Setter for rank
	public void setRank(int rank) {
		this.rank = rank;
	}

	//Getter for name
	public String getName() {
		return name;
	}

	//Setter for name
	public void setName(String name) {
		this.name = name;
	}

	//Getter for team
	public int getTeam() {
		return team;
	}

	//Setter for team
	public void setTeam(String team) {
		this.team = team;
	}

	//Getter for Average
	public float getAvg() {
		return avg;
	}

	//Setter for Average
	public void setAvg(float avg) {
		this.avg = avg;
	}

	//Getter for RC
	public float getRc() {
		return rc;
	}

	//Setter for RC
	public void setRc(float rc) {
		this.rc = rc;
	}

	//Getter for RC27
	public float getRc27() {
		return rc27;
	}

	//Setter for RC27
	public void setRc27(float rc27) {
		this.rc27 = rc27;
	}

	//Getter for ISOP
	public float getIsop() {
		return isop;
	}

	//Setter for ISOP
	public void setIsop(float isop) {
		this.isop = isop;
	}

	//Getter for SECA
	public float getSeca() {
		return seca;
	}

	//Setter for SECA
	public void setSeca(float seca) {
		this.seca = seca;
	}

	//Getter for GB
	public float getGb() {
		return gb;
	}

	//Setter for GB
	public void setGb(float gb) {
		this.gb = gb;
	}

	//Getter for FB
	public float getFb() {
		return fb;
	}

	//Setter for FB
	public void setFb(float fb) {
		this.fb = fb;
	}

	//Getter for GF
	public float getGf() {
		return gf;
	}

	//Setter for GF
	public void setGf(float gf) {
		this.gf = gf;
	}

	//Getter for ABHR
	public float getAbhr() {
		return abhr;
	}

	//Setter for ABHR
	public void setAbhr(float abhr) {
		this.abhr = abhr;
	}

	//Getter for BBPA
	public float getBbpa() {
		return bbpa;
	}

	//Setter for BBPA
	public void setBbpa(float bbpa) {
		this.bbpa = bbpa;
	}

	//Getter for BBK
	public float getBbk() {
		return bbk;
	}

	//Setter for BBK
	public void setBbk(float bbk) {
		this.bbk = bbk;
	}



}